#include<iostream>
using namespace std;

int maximum1(int x,int y){
    
    if(x>y)
    return x;
    else
    return y;
}
  int maximum2(int u,int v){
    
    if(u>v)
    return u;
    else
    return v; } 
    int greatest(int p,int k){
    
    if(p>k)
    return p;
    else
    return k; } 

int main(){
    int a,b,c,d;
    
    
    cin>>a;
    
    cin>>b;
    
    cin>>c;
    
    cin>>d;
    int p =maximum1(a,b);
    int k =maximum2(c,d);
    int t=greatest(p,k);
    cout<<t<<endl;
    
    return 0;
}



