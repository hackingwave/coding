#include<iostream>
using namespace std;
int main()
{
        
        int q;
        cin>>q;
        int a[q];
        for(int i=0;i<q;i++)
        {
                
               cin>>a[i];
               
               if(a[i]<=37)
               {
                       cout<<a[i]<<endl;
                       
               }
               
               else if( a[i]%5==0)
               {
                       cout<<a[i]<<endl;
               }
                
                else if(a[i]%5==4)
                {
                        cout<<a[i]+1<<endl;
                }
                else if (a[i]%5==3)
                {
                        cout<<a[i]+2<<endl;
                }
                else
                {
                        cout<<a[i]<<endl;
                }
                
        }
        
        
        
        
        
        return 0;
}
