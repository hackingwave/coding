#include<iostream>
#include<string>
using namespace std;
int main()
{
     int count =1;
     string s;
     cin>>s;
     for(int i=0;i<s.size();i++)
     {
             switch(s[i]){
                 case 'A'...'Z':
                 count++;
             }
             
     }
        
        cout<<count;
        return 0;
}
